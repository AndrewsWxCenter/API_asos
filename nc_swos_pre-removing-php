$today = $data[STATION][0][OBSERVATIONS][ceiling_value_1][date_time];
$tostr = strval($today);
//echo $today;



//KRDU
$file  = fopen('KRDU_climo.csv', 'r');
while (($line = fgetcsv($file)) !== FALSE) {
  //$line is an array of the csv elements
  $arrayKRDU[] = $line;
}
fclose($file);
//print_r($arrayKRDU);

$keyKRDU = array_search($tostr, array_column($arrayKRDU, 0));
$highKRDU = $arrayKRDU[$keyKRDU][1];
$lowKRDU  = $arrayKRDU[$keyKRDU][2];
/*
echo $keyKRDU;
echo $highKRDU;
echo $lowKRDU;
*/



